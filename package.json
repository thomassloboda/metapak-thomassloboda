{
  "name": "metapak-thomassloboda",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "metapak": {
    "configs": [
      "eslint",
      "jsarch",
      "readme"
    ],
    "data": {
      "files": "src/**/*.js"
    }
  },
  "scripts": {
    "architecture": "jsarch src/**/*.js > ARCHITECTURE.md",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "cz": "env NODE_ENV=${NODE_ENV:-cli} git cz",
    "lint": "eslint src/**/*.js",
    "metapak": "metapak",
    "postinstall": "npm run metapak --silent || exit 0",
    "prettier": "prettier --write src/**/*.js",
    "preversion": "npm t && npm run lint && npm run metapak -s",
    "test": "exit 0",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "keywords": [],
  "author": "Thomas Sloboda",
  "repository": {
    "type": "git",
    "url": "https://github.com/thomassloboda/metapak-thomassloboda"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "1.0.0",
    "metapak": "^1.0.2"
  },
  "peerDependencies": {
    "metapak": "^1.0.2"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.5",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-prettier": "^2.3.1",
    "jsarch": "^1.2.5",
    "prettier": "^1.8.2"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "commitizen",
      "cz-conventional-changelog",
      "conventional-changelog-cli",
      "eslint",
      "eslint-config-prettier",
      "prettier"
    ]
  }
}